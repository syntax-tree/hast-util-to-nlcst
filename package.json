{
  "name": "hast-util-to-nlcst",
  "version": "1.2.8",
  "description": "hast utility to transform to nlcst",
  "license": "MIT",
  "keywords": [
    "unist",
    "hast",
    "hast-util",
    "util",
    "utility",
    "rehype",
    "retext",
    "nlcst",
    "html",
    "natural",
    "language"
  ],
  "repository": "syntax-tree/hast-util-to-nlcst",
  "bugs": "https://github.com/syntax-tree/hast-util-to-nlcst/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "hast-util-embedded": "^2.0.0",
    "hast-util-is-element": "^2.0.0",
    "hast-util-phrasing": "^2.0.0",
    "hast-util-to-string": "^1.0.0",
    "hast-util-whitespace": "^2.0.0",
    "nlcst-to-string": "^3.0.0",
    "unist-util-position": "^4.0.0",
    "vfile-location": "^3.1.0"
  },
  "devDependencies": {
    "c8": "^7.0.0",
    "is-hidden": "^2.0.0",
    "parse-dutch": "^5.0.0",
    "parse-english": "^5.0.0",
    "parse-latin": "^5.0.0",
    "prettier": "^2.0.0",
    "rehype": "^11.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "vfile": "^4.0.0",
    "xo": "^0.39.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node test/index.js",
    "test-coverage": "c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov node test/index.js",
    "test": "npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "no-var": "off",
      "prefer-arrow-callback": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
