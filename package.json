{
  "name": "hast-util-to-nlcst",
  "version": "1.2.8",
  "description": "hast utility to transform to nlcst",
  "license": "MIT",
  "keywords": [
    "unist",
    "hast",
    "hast-util",
    "util",
    "utility",
    "rehype",
    "retext",
    "nlcst",
    "html",
    "natural",
    "language"
  ],
  "repository": "syntax-tree/hast-util-to-nlcst",
  "bugs": "https://github.com/syntax-tree/hast-util-to-nlcst/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "hast-util-embedded": "^1.0.0",
    "hast-util-is-element": "^1.0.0",
    "hast-util-phrasing": "^1.0.0",
    "hast-util-to-string": "^1.0.0",
    "hast-util-whitespace": "^1.0.0",
    "nlcst-to-string": "^2.0.0",
    "unist-util-position": "^3.0.0",
    "vfile-location": "^3.1.0"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "is-hidden": "^1.0.0",
    "negate": "^1.0.0",
    "nyc": "^15.0.0",
    "parse-dutch": "^4.0.0",
    "parse-english": "^4.0.0",
    "parse-latin": "^4.0.0",
    "prettier": "^2.0.0",
    "rehype": "^11.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "tinyify": "^3.0.0",
    "vfile": "^4.0.0",
    "xo": "^0.34.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "build-bundle": "browserify . -s hastUtilToNlcst -o hast-util-to-nlcst.js",
    "build-mangle": "browserify . -s hastUtilToNlcst -o hast-util-to-nlcst.min.js -p tinyify",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run format && npm run build && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "unicorn/explicit-length-check": "off",
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prefer-optional-catch-binding": "off"
    },
    "ignores": [
      "hast-util-to-nlcst.js"
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
