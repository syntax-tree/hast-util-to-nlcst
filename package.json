{
  "name": "hast-util-to-nlcst",
  "version": "2.2.0",
  "description": "hast utility to transform to nlcst",
  "license": "MIT",
  "keywords": [
    "unist",
    "hast",
    "hast-util",
    "util",
    "utility",
    "rehype",
    "retext",
    "nlcst",
    "html",
    "natural",
    "language"
  ],
  "repository": "syntax-tree/hast-util-to-nlcst",
  "bugs": "https://github.com/syntax-tree/hast-util-to-nlcst/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/hast": "^2.0.0",
    "@types/nlcst": "^1.0.0",
    "@types/unist": "^2.0.0",
    "hast-util-embedded": "^2.0.0",
    "hast-util-is-element": "^2.0.0",
    "hast-util-phrasing": "^2.0.0",
    "hast-util-to-string": "^2.0.0",
    "hast-util-whitespace": "^2.0.0",
    "nlcst-to-string": "^3.0.0",
    "unist-util-position": "^4.0.0",
    "vfile": "^5.0.0",
    "vfile-location": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "c8": "^7.0.0",
    "hast-util-from-html": "^1.0.0",
    "is-hidden": "^2.0.0",
    "parse-dutch": "^6.0.0",
    "parse-english": "^6.0.0",
    "parse-latin": "^6.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "xo": "^0.53.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test/index.js",
    "test-coverage": "c8 --check-coverage --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "overrides": [
      {
        "files": "test/**/*.js",
        "rules": {
          "no-await-in-loop": 0
        }
      }
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true
  }
}
